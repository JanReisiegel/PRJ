{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "IdentityServer": {
    "Clients": {
      "OAuthServerRP": {
        "Profile": "IdentityServerSPA",
        "RedirectUri": "/oidc-callback",
        "LogoutUri": "/oidc-signout-callback",
        "AlwaysIncludeUserClaimsInIdToken": true
      }
    }
  },
  "AllowedHosts": "*"
}
